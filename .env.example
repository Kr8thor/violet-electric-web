# Violet React App Environment Variables Template
# Copy this to .env and fill in your actual values

# WordPress API Key (get this from WordPress Admin → Settings → Violet API Key)
VITE_VIOLET_API_KEY=your_api_key_here

# WordPress Backend URLs
VITE_WORDPRESS_URL=https://wp.violetrainwater.com
VITE_WORDPRESS_REST_URL=https://wp.violetrainwater.com/wp-json

# Netlify Settings
VITE_NETLIFY_URL=https://lustrous-dolphin-447351.netlify.app

# Development Settings
VITE_DEBUG_MODE=true
VITE_ENABLE_LOGGING=true

# Build Settings
VITE_APP_VERSION=1.0.0
VITE_BUILD_DATE=2025-01-17
